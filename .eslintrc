{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "prettier",
    "plugin:cypress/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "singleQuote": true, "endOfLine": "auto" }],
    "import/first": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
